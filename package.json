{
	"name": "@spmeesseman/extjs-pkg-webworker",
	"displayName": "ExtJS Package Wrapper for WebWorker",
	"description": "ExtJS Package Wrapper for WebWorker",
	"publisher": "spmeesseman",
	"version": "1.0.2",
	"license": "MIT",
	"devDependencies": {
		"@semantic-release/changelog": "^3.0.2",
		"@semantic-release/exec": "^3.3.0",
		"@semantic-release/git": "^7.0.8",
		"commitizen": "^3.0.7",
		"conventional-changelog-spm": "^1.2.3",
		"cz-conventional-changelog": "^2.1.0",
		"semantic-release": "^15.13.12"
	},
	"scripts": {
		"semantic-release": "semantic-release --no-ci",
		"semantic-release-dry": "semantic-release --dry-run"
	},
	"config": {
		"commitizen": {
			"path": "cz-conventional-changelog"
		}
	},
	"publishConfig": {
		"access": "public"
	},
	"files": [
		"src"
	],
	"sencha": {
		"name": "webworker",
		"namespace": "Ext",
		"framework": "ext",
		"type": "code",
		"creator": "spmeesseman",
		"summary": "Package wrapper for WebWorker",
		"detailedDescription": "Package wrapper for WebWorker",
		"version": "1.0.0.0",
		"compatVersion": "1.0.0",
		"format": "1",
		"slicer": null,
		"output": "${framework.dir}/build/packages/${package.name}",
		"local": true,
		"resource": {
			"paths": ""
		},
		"sass": {
			"namespace": "Ext",
			"etc": [
				"${package.dir}/sass/var"
			],
			"var": [
				"${package.dir}/sass/var"
			],
			"src": [
				"${package.dir}/sass/src",
				"${package.dir}/sass/src/main.scss"
			]
		},
		"classpath": [
			"${package.dir}/src"
		],
		"overrides": [
			"${package.dir}/overrides"
		],
		"requires": [
			"core"
		],
		"ignore": [
			"(^|/)CVS(/?$|/.*?$)"
		]
	}
}
